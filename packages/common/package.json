{
  "name": "@ts-journey/common",
  "version": "1.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "run-p watch:tsc watch:types:alias",
    "build": "run-s clean tsc types:alias",
    "tsc": "tsc",
    "watch:tsc": "tsc -w",
    "types:alias": "tsc-alias",
    "watch:types:alias": "tsc-alias -w",
    "clean": "rm -rf ./dist && rm -rf ./build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "aws-sdk": "2.965.0",
    "json-beautify": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.7",
    "eslint": "^8.46.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths-jest": "^0.0.1",
    "typescript": "^5.2.2"
  }
}
